{
    "code-block-style": {
      "style": "fenced"
    },
    "code-fence-style": {
      "style": "consistent"
    },
    "emphasis-style": {
      "style": "underscore"
    },
    "extended-ascii": {
      "ascii-only": true
    },
    "fenced-code-language": {
      "allowed_languages": [
        "bash",
        "html",
        "javascript",
        "json",
        "markdown",
        "php",
        "text"
      ],
      "language_only": true
    },
    "heading-style": {
      "style": "atx"
    },
    "hr-style": {
      "style": "---"
    },
    "line-length": {
      "line_length": 240,
      "strict": false,
      "code_blocks": false
    },
    "link-image-style": {
      "collapsed": false,
      "shortcut": true,
      "url_inline": false
    },
    "no-duplicate-heading": {
      "siblings_only": true
    },
    "ol-prefix": {
      "style": "ordered"
    },
    "reference-links-images": {
      "shortcut_syntax": true
    },
    "strong-style": {
      "style": "asterisk"
    },
    "ul-style": {
      "style": "dash"
    }
  }
